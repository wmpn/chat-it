generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String @db.VarChar(191)
  email String @unique @db.VarChar(191)
  provider String @db.VarChar(100)
  image String? 
  oauth_id String 
  created_at DateTime @default(now())

  @@map("users")
}